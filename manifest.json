{
    "manifest_version": 3,
    "name": "LinkedInSaves",
    "description": "A Chrome extension to organize LinkedIn saved posts into categories",
    "version": "1.0",
    "permissions": [
      "storage",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "assets/linkedInSaves-16x16.png",
      "48": "assets/linkedInSaves-48x48.png",
      "128": "assets/linkedInSaves-128x128.png"
    },
  "browser_specific_settings": {
      "gecko": {
        "id": "linkedinsaves@gmail.com",
        "strict_min_version": "58.0"
      }
    }
  }
  