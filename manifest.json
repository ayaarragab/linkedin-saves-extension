{
    "manifest_version": 3,
    "name": "LinkedInSaves",
    "description": "A Chrome extension to organize LinkedIn saved posts into categories",
    "version": "1.0",
    "permissions": [
      "tabs",
      "storage",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["https://*.linkedin.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "assets/linkedInSaves-16x16.png",
      "48": "assets/linkedInSaves-48x48.png",
      "128": "assets/linkedInSaves-128x128.png"
    }
  }
  